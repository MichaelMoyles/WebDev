  <select id="state" onchange="show(this)">
        <option value="">Choose State</option>
            </select>
            
            
        <select id="party" onchange="show(this)">
        <option value="">Choose Party</option>
            </select>
            
        <select id="rank" onchange="show(this)">
        <option value="">Choose Rank</option>
            </select>
            
            
            
            
            
            
            
 let dropdown = document.getElementById('state');
dropdown.length = 0;

let defaultOption = document.createElement('option');
defaultOption.text = 'Choose State';

dropdown.add(defaultOption);
dropdown.selectedIndex = 0;


function stateDropdown() {
    var senators = parsedObj.objects;
    var senatorState = senators[i].state;
    
    for (let i = 0; i < senators.length; i++) {
    let option;
      option = document.createElement('option');
      option.text = senatorState[i];
      option.value = senatorState[i];
      dropdown.add(option);
    
    document.getElementById('state').innerHTML
    }

    }

    function show(ele) {
        // Get the selected value from <select> element and show it.
        let msg = document.getElementById('msg');
        msg.innerHTML = 'Selected State: <b>' + ele.options[ele.selectedIndex].text + '</b> </br>';
    }
           
